<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Budget</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="https://unpkg.com/ionicons@4.5.10-0/dist/css/ionicons.min.css">
    <style> body { background-color: #d6ffed; } .navbar { background-color: #a4f880; font-family: 'French Script MT'; font-size: 30px; } .footer { font-family: Castellar; background-color: #c2ffa9; } </style>
</head>
<body>
    <header>
        @*<img src="~/top.png" width="150" height="50" />*@
        @*<table class="center">
            <tr>
                <td>*@
        <center>
            <div class="container portrait">
                <img src="~/top.png" width="800" height="230">
            </div>
        </center>
                @*</td>
            </tr>
            <tr><td>
        </table>*@
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Budget" asp-action="Index">Budget</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <partial name="_LoginPartial" />
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link ion-md-home text-dark" asp-area="" asp-controller="Budget" asp-action="Index">Home</a>
                        </li>
                        @*<li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Budget" asp-action="Privacy">Privacy</a>
                        </li>*@
                        <li class="nav-item">
                            <a class="nav-link ion-md-people text-dark" asp-area="" asp-controller="Members" asp-action="GetMembers">Members</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link ion-md-chatboxes text-dark" asp-area="" asp-controller="Messages" asp-action="GetMessages">Messages</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link ion-md-wallet text-dark" asp-area="" asp-controller="Checks" asp-action="Index">Checks</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
</header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2019 - KTUSTPPBiudzetas - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
